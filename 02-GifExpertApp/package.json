    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "postcss-import": "^14.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.2.4"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
