  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isandeepbansal/react-starter-kit/issues"
  },
  "homepage": "https://github.com/isandeepbansal/react-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react-hooks": "^7.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "autoprefixer": "^10.3.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.30.0",
